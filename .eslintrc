{
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "no-console": "error",
    "no-return-await": "off",
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-param-reassign": ["error", { "props": false }]
  }
}
